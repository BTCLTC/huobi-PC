    "require-dir": "^1.0.0",
    "sinon": "^4.3.0",
    "sortablejs": "^1.7.0",
    "spectron": "^3.8.0",
    "style-loader": "^0.20.2",
    "tree-kill": "^1.2.0",
    "tslint": "^5.9.1",
    "tslint-config-airbnb": "^5.7.0",
    "tslint-loader": "^3.5.3",
    "ts-loader": "^3.5.0",
    "typescript": "^2.7.2",
    "typescript-formatter": "^7.1.0",
    "url-loader": "^0.6.2",
    "vue-awesome": "^2.3.4",
    "vue-class-component": "^6.2.0",
    "vue-focus": "^2.1.0",
    "vue-hot-reload-api": "^2.3.0",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^14.1.1",
    "vue-property-decorator": "^6.0.0",
    "vue-style-loader": "^4.0.2",
    "vue-template-compiler": "^2.5.13",
    "webpack": "^3.11.0",
    "webpack-dev-middleware": "^2.0.5",
    "webpack-dev-server": "^2.11.1",
    "webpack-hot-middleware": "^2.21.0",
    "webpack-merge": "^4.1.1",
    "workerize-loader": "^1.0.1"
  },
  "ava": {
    "files": [
      "test/e2e/index.js"
    ],
    "concurrency": 5,
    "failWithoutAssertions": false,
    "powerAssert": false,
    "require": [
      "babel-register"
    ]
  },
  "engines": {
    "node": ">=8.9.3"
  }
}
