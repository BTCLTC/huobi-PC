{
  "name": "lulumi-browser",
  "productName": "Lulumi-browser",
  "version": "1.3.3",
  "electronVersion": "2.0.0-beta.1",
  "description": "Lulumi-browser is a light weight browser coded with vue and electron",
  "main": "./dist/main.js",
  "scripts": {
    "build": "node .electron-vue/build.js",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:darwin": "cross-env BUILD_TARGET=darwin node .electron-vue/build.js",
    "build:linux": "cross-env BUILD_TARGET=linux node .electron-vue/build.js",
    "build:mas": "cross-env BUILD_TARGET=mas node .electron-vue/build.js",
    "build:win32": "cross-env BUILD_TARGET=win32 node .electron-vue/build.js",
    "dev": "node .electron-vue/dev-runner.js",
    "fmt": "tsfmt -r --baseDir src/",
    "lint": "yarn run fmt && tslint -p src/tsconfig.json -c tslint.json src/**/*.ts",
    "lint:fix": "yarn run fmt && tslint -p src/tsconfig.json -c tslint.json --fix src/**/*.ts",
    "pack": "yarn run pack:main && yarn run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "pack:e2e": "yarn run pack:main:e2e && yarn run pack:renderer:e2e",
    "pack:main:e2e": "cross-env TEST_ENV=e2e NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer:e2e": "cross-env TEST_ENV=e2e NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "e2e": "yarn run pack:e2e && ava",
    "unit:renderer": "karma start test/unit/renderer.karma.conf.js",
    "unit:about": "karma start test/unit/about.karma.conf.js",
    "unit": "yarn run unit:renderer && yarn run unit:about",
    "test": "yarn run unit && yarn run e2e",
    "postinstall": "yarn run lint:fix"
  },
  "author": "Boik Su <boik@tdohacker.org>",
  "license": "Apache-2.0",
  "dependencies": {
    "axios": "^0.18.0",
    "collect.js": "^4.0.11",
    "css-element-queries": "^1.0.1",
    "electron-localshortcut": "^3.1.0",
    "electron-process-manager": "^0.5.1",
    "fuse.js": "^3.2.0",
    "lulumi": "^0.0.23",
    "nanoid": "^1.0.2",
    "node-localstorage": "^1.3.0",
    "vue": "^2.5.13",
    "vue-electron": "^1.0.6",
    "vue-i18n": "^7.4.2",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.59",
    "@types/mocha": "^2.2.48",
    "@types/webpack": "^3.8.8",
    "@types/webpack-env": "^1.13.5",
    "@types/webpack-hot-middleware": "^2.16.2",
    "ava": "^0.25.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-closure-elimination": "^1.3.0",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "babel-minify-webpack-plugin": "^0.3.0",
    "cfonts": "^1.1.3",
    "chai": "^4.1.2",
    "chalk": "^2.3.1",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.9",
    "del": "^3.0.0",
    "devtron": "^1.4.0",
    "electron": "2.0.0-beta.1",
    "electron-debug": "1.4.0",
    "electron-devtools-installer": "^2.2.3",
    "electron-packager": "^11.0.1",
    "electron-rebuild": "^1.7.3",
    "electron-unhandled": "^1.0.0",
    "electron-util": "^0.6.0",
    "element-ui": "^2.2.0",
    "eslint": "^4.18.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-import-resolver-webpack": "^0.8.4",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^4.0.2",
    "eslint-plugin-import": "^2.9.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.9",
    "git-rev-sync": "^1.10.0",
    "html-webpack-plugin": "^2.30.1",
